campaigns/${props.address}/requests`);\n  };\n\n  const onFinalize = async () => {\n    const campaign = Campaign(props.address);\n    const accounts = await web3.eth.getAccounts();\n    await campaign.methods.finalizeRequest(props.id).send({\n      from: accounts[0],\n    });\n    Route.replaceRoute(`/campaigns/${props.address}/requests`);\n  };\n\n  return (\n    <Row>\n      <Cell>{props.id}</Cell>\n      <Cell>{props.request.description}</Cell>\n      <Cell>{web3.utils.fromWei(props.request.value, \"ether\")}</Cell>\n      <Cell>{props.request.recipient}</Cell>\n      <Cell>\n        {props.request.aprovalCount}/{props.aproversCount}\n      </Cell>\n      <Cell>\n        <Button color=\"green\" basic onClick={onApprove}>\n          approve\n        </Button>\n      </Cell>\n      <Cell>\n        <Button color=\"teal\" basic onClick={onFinalize}>\n          Finalize\n        </Button>\n      </Cell>\n    </Row>\n  );\n};\n\nexport default RequestRow;\n"]},"metadata":{},"sourceType":"module"}